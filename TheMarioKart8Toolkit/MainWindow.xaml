        <Grid.Background>
            <ImageBrush ImageSource="Images/BG.png"/>
        </Grid.Background>
        <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Width="940" Height="35" Background="#00000000" MouseDown="DragWindow"></Label>
